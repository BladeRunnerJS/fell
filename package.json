{
  "name": "fell",
  "version": "0.1.0",
  "description": "A simple javascript logging framework.",
  "homepage": "http://BladeRunnerJS.github.io/fell",
  "license": "MIT",
  "dependencies": {
    "emitr": "^0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BladeRunnerJS/fell.git"
  },
  "main": "./src/fell.js",
  "scripts": {
    "prepublish": "browserify src/fell.js -s fell -o dist/fell.js",
